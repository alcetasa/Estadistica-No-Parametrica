datos=read.delim("clipboard")
be=subset(datos,empresa=="Be")
tabla<-table(be$combustible)
rela<-c(5,4,7)
prob=rela/sum(rela)
sum(rela)
# Método exacto
library(RVAideMemoire)
#Pasar la tabla inicial a vector
tabla1<-as.vector(tabla)
multinomial.test(tabla1,prob)
